  <CharacterStyleRange AppliedCharacterStyle="$ID/NormalCharacterStyle">
    <Content>Caswell, Michelle, Vo Dang, Thuy and Sutherland, Tonia. ‘Community Archives Lab’, Community Archives Lab UCLA, n.d. </Content>
  </CharacterStyleRange>
