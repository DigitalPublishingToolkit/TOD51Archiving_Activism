  </CharacterStyleRange>
  <CharacterStyleRange AppliedCharacterStyle="CharacterStyle/Italic">
    <Content>Urgent Archives: Enacting Liberatory Memory Work</Content>
  </CharacterStyleRange>
  <CharacterStyleRange AppliedCharacterStyle="$ID/NormalCharacterStyle">
    <Content>, London-New York: Routledge, 2021.</Content>
  </CharacterStyleRange>
</ParagraphStyleRange>
<Br />
